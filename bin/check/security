variant="${SECURITY:=local-php-security-checker}"

printf "${check}>>> running ${check_name}security${check} (%s) check on ${check_name}%s${reset}\n" "${variant}" "${NAME}"

"${BIN_DIR}/check/_variant/security-${variant}" $*
