. "$(dirname $0)/../config"

variant="${FIX_CODE_STYLE:=phpcbf}"

printf "${check}>>> running ${check_name}code-style${check} (%s) fix on ${check_name}%s${reset}\n" "${variant}" "${NAME}"

"${BIN_DIR}/fix/_variant/code-style-${variant}" $*
